div.header-panel div.span12.masthead a.menu .stripes.last { margin-bottom:0; }

/*div.slide-menu { background-color:#161a1c; /*width:220px; width:auto !important;  position:fixed; right:-220px; top:0; bottom:0; z-index:99999999; margin-right:-101px; }*/
